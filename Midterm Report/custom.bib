	keywords = {Economic growth, Government size, Panel Granger causality, Wagner's law},
	pages = {804--817},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\ijyli\\Zotero\\storage\\8BV7LYVY\\Wu et al. - 2010 - The impact of government expenditure on economic g.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\ijyli\\Zotero\\storage\\U3ILN8AQ\\S0161893810000463.html:text/html},
