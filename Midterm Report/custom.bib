	note = {Publisher: The University of Chicago Press},
	pages = {397--444},
	file = {Full Text PDF:C\:\\Users\\ijyli\\Zotero\\storage\\2ZRNYY75\\Dynan et al. - 2004 - Do the Rich Save More.pdf:application/pdf},
