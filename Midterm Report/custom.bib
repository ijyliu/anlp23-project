	year = {1997},
	doi = {10.1007/978-94-011-4942-6_13},
	keywords = {Emotional Exhaustion, Maslach Burnout Inventory, Personal Accomplishment, School Reform, Secondary School Teacher},
	pages = {459--498},
}

